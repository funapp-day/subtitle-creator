{
  "extends": "next/core-web-vitals",
  "rules": {
    "comma-dangle": [
      "error",
      {
        "arrays": "always",
        "objects": "always",
        "imports": "never",
        "exports": "never",
        "functions": "never"
      }
    ],
    "semi": ["error", "always"],
    "indent": ["error", 2],
    "eol-last": ["error", "always"],
    "quotes": ["error", "double"],
    "react/self-closing-comp": [
      "error",
      {
        "component": true,
        "html": true
      }
    ],
  }
}
